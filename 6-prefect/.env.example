# Copy this file to .env and fill in the blank values.
# Stage 1 (manual run) only requires Prefect server connectivity.
# Stage 2 (deployment demo) requires WEATHER_API_KEY for weather ETL.

COINCAP_API_KEY=YOUR_KEY
WEATHER_API_KEY=YOUR_KEY

# Postgres values are only needed for Postgres ETL variants (02/03 flows).
DB_HOST=postgres
DB_PORT=5432
DB_USER=prefect
DB_PASSWORD=prefect
DB_NAME=prefect

# Prefect API endpoint used by bootstrap/worker/cli services
PREFECT_API_URL=http://prefect-server:4200/api
